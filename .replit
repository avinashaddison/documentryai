modules = ["nodejs-20", "python-3.11", "web"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"
packages = ["ffmpeg", "ffmpeg-full", "freetype", "imagemagickBig", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[[ports]]
localPort = 5000
externalPort = 80

[env]
PORT = "5000"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
publicDir = "dist/public"
run = ["node", "./dist/index.cjs"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[agent]
mockupState = "FULLSTACK"
integrations = ["javascript_anthropic_ai_integrations:2.0.0", "javascript_object_storage:2.0.0"]

[objectStorage]
defaultBucketID = "replit-objstore-b8839db5-b47e-4fe0-89a6-b6bbf4d9e877"

[userenv]

[userenv.shared]
AI_INTEGRATIONS_ANTHROPIC_BASE_URL = "http://localhost:1106/modelfarm/anthropic"
AI_INTEGRATIONS_ANTHROPIC_API_KEY = "_DUMMY_API_KEY_"
DEFAULT_OBJECT_STORAGE_BUCKET_ID = "replit-objstore-b8839db5-b47e-4fe0-89a6-b6bbf4d9e877"
PUBLIC_OBJECT_SEARCH_PATHS = "/replit-objstore-b8839db5-b47e-4fe0-89a6-b6bbf4d9e877/public"
PRIVATE_OBJECT_DIR = "/replit-objstore-b8839db5-b47e-4fe0-89a6-b6bbf4d9e877/.private"
